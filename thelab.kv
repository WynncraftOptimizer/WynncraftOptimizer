<ScreenNav>:
    BoxLayout:
        orientation: "vertical"
        MDToolbar:
            id: toolbar
            pos_hint: {"top": 1}
            md_bg_color: 0.1, 0.1, 0.2, 1
            font_name: app.fontse
            elevation: 10
            title: "WynnOptimizer"
            left_action_items: [["menu", lambda x: nav_drawer.set_state("open")]]
        MDNavigationLayout:
            x: toolbar.height
            ScreenManager:
                id:screen_manager
                BoxLayoutExample:
                    screen_manager: screen_manager
                    name: "input"
                OutputScreen:
                    screen_manager: screen_manager
                    name: "output"
                SavedItemsScreen:
                    screen_manager: screen_manager
                    name: "localstorage"
                SettingsScreen:
                    screen_manager: screen_manager
                    name: "settings"
            MDNavigationDrawer:
                id: nav_drawer
                ContentNavigationDrawer:
                    screen_manager: screen_manager
                    nav_drawer: nav_drawer
